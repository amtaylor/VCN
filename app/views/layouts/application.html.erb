				<div class="col-md-12">
					<%= yield %>
				</div>
			</div>
		</div>

	</div>	<!-- end wrap -->
	<%= render 'layouts/footer' %>

	<%= render 'layouts/authentication' %>	
	
	<!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
