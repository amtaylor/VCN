	  <script src="../../assets/js/html5shiv.js"></script>
	  <script src="../../assets/js/respond.min.js"></script>
	<![endif]-->
  

  	<%= render 'layouts/ga' %>	
</head>
<body>

	<div id="wrap"> <!-- added this to get the sticky footer-->


		<%= render 'layouts/header' %>

		<div class="container">
